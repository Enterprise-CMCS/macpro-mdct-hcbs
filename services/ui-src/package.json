{
  "name": "ui-src",
  "version": "1.0.0",
  "license": "CC0-1.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "coverage": "jest --coverage",
    "test": "jest --setupFiles dotenv/config",
    "watchTest": "jest --setupFiles dotenv/config --watch",
    "a11y": "pa11y --runner axe --runner htmlcs"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.10.9",
    "@chakra-ui/transition": "^2.1.0",
    "@cmsgov/design-system": "^13.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "aws-amplify": "^6.15.8",
    "core-js": "^3.47.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.25",
    "html-react-parser": "^5.2.10",
    "launchdarkly-react-client-sdk": "^3.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.30.2",
    "yup": "^1.7.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-jest": "^30.2.0",
    "dotenv": "^17.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "pa11y": "^9.0.1",
    "pa11y-runner-htmlcs": "^2.0.1",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "jest": {
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!src/components/index.ts*",
      "src/routes/**/*.{ts,tsx}",
      "!src/routes/index.tsx",
      "!src/types/index.ts*",
      "src/utils/**/*.{ts,tsx}",
      "!src/utils/testing/*",
      "!src/utils/index.ts*",
      "!src/utils/**/index.ts*"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg|mp4|webm|wav|mp3|m4a)$": "<rootDir>/src/utils/testing/mockAsset.ts"
    },
    "setupFiles": [],
    "setupFilesAfterEnv": [
      "<rootDir>/src/utils/testing/setupJest.tsx"
    ],
    "globals": {
      "_env_": {}
    },
    "testEnvironment": "jest-environment-jsdom"
  }
}
